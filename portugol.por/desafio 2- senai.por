programa {
  funcao inicio() {
    inteiro valor 

    escreva("Digite um valor: ")
    leia(valor)

    se (valor == 10){
      escreva("É igual a  10.")
    }
    se ( valor > 10)  
    {
      escreva("É maior que 10.")
    }
    se (valor < 10){
      escreva("O número é menor a 10")
    }
  }
}
